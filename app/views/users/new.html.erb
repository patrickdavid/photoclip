<h1>Sign Up</h1>

<%= render('layouts/errors', :object => @user) %>

<%= bootstrap_form_for(@user, :url => users_path, :html => { :multipart => true }) do |f| %>
  <%= f.email_field :email %>
  <%= f.password_field :password %>
  <%= f.password_field :password_confirmation %>
  <%= f.file_field :avatar %>
  <%= f.submit "Sign Up" %>
<% end %>


