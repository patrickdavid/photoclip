

<h1>Welcome to the world wide web of photos</h1>
<div class="container">
  <div id="photogrid"class="row">
    <% @photos.each do |photo| %>
      <%= image_tag photo.photo.url(:medium) %>

      <p><%= link_to "Edit", edit_photo_path(photo), :class => "btn btn-default" %></p>
      <p><%= link_to "Delete", photo_path(photo),
                         :data => {:confirm => "You sure?",
                                   :method => "delete"},
                         :class => "btn btn-danger" %></p>
      <% end %>
  </div>
</div>
